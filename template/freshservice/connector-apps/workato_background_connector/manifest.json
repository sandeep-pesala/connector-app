{
  "platform-version": "2.3",
  "product": {
    "freshservice": {
      "location": {
        "full_page_app": {
          "url": "index.html",
          "icon": "assets/images/icon.svg"
        }
      },
      "requests": {
        "fetchToken": {},
        "initTenantCreation": {},
        "fetchTenant": {},
        "listRecipes": {},
        "listConnections": {},
        "startRecipe": {},
        "stopRecipe": {},
        "updateConnection": {},
        "triggerEndpoint": {},
        "fetchAlertConfig": {},
        "updateAlertConfig": {},
        "fetchAgents": {}
      },
      "functions": {
        "getJwtToken": {
          "timeout": 10
        },
        "getAllRecipes": {
          "timeout": 10
        },
        "startRecipe": {
          "timeout": 10
        },
        "stopRecipe": {
          "timeout": 10
        }
      },
      "events": {
        "onAppInstall": {
          "handler": "onAppInstallCallback"
        },
        "onAppUninstall": {
          "handler": "onAppUninstallCallback"
        }
      }
    }
  },
  "engines": {
    "node": "18.20.4",
    "fdk": "9.2.0"
  },
  "dependencies": {
    "jsonwebtoken": "8.1.1"
  }
}
